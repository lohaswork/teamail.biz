!function($) {

  App.page.UnreadNumberPage = function (){
    this.init();
  }
  App.page.UnreadNumberPage.prototype = {
    init: function(){
      var self = this;
      <% if Rails.env.production? || Rails.env.staging? %>
        $(document).ready(
          function(){
            setInterval(function(){
              $('#unread-number-query').load('/get-unread-number');
            }, 60000);
          }
        )
      <% end %>
    }
  }

}(window.jQuery);
