!function($) {

  App.page.UnreadNumberPage = function (){
    this.init();
  }
  App.page.UnreadNumberPage.prototype = {
    init: function(){
      var self = this;
      <% unless Rails.env.test? %>
        $(document).ready(
          function(){
            setInterval(function(){
              $('#unread-num-query').load('/get_unread_num');
            }, 60000);
          }
        )
      <% end %>
    }
  }

}(window.jQuery);
