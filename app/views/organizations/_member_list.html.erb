  <div class='row' id="member-list">
    <div><%= current_user.email_name %></div>
    <%- colleagues.try(:each) do |user|%>
      <div class="member-name">
        <%- if user.active_status %>
          <%= user.email_name %>
        <%- else %>
          <%= user.email %>
        <% end %>
      </div>
      <div class="member-container">
        <%- if currently_admin? %>
          <%= link_to delete_member_path(:user => user), :method => :post, :remote => true, :class=>"center" do %>
            <span>删除该成员</span>
          <% end %>
        <% end %>
      </div>
    <% end %>
  </div>
